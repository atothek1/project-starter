{
  "name": "@atothek/project-starter",
  "version": "1.0.0",
  "repository": "https://github.com/atothek1/project-starter.git",
  "author": "Alexander KÃ¤mmerer",
  "license": "MIT",
  "private": true,
  "packageManager": "yarn@3.2.0",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "ws:new": "sh -c 'yarn packages/${0} init && yarn'",
    "ws:add": "sh -c 'yarn workspace ${0} add ${1}'",
    "ws:add-d": "sh -c 'yarn workspace ${0} add -D ${1}'",
    "ws:run": "sh -c 'yarn workspace ${0} run ${1}'"
  },
  "devDependencies": {
    "@types/node": "^17.0.31",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  }
}
